# # # # # # # # # # # # # #
# IMML schema definitions #
# IMML is the immatériel.fr preferred exchange format for metadata #
# 
# *Unsupported elements* : when an element is not supported at the emitter end, it must
# contain an attribute "unsupported" with value "true". Otherwise, it may explicitly 
# indicate support with a "false" value, but absence of this att is ok too.

# default namespace = "http://ns.immateriel.fr/imml"

namespace a = "http://relaxng.org/ns/compatibility/annotations/1.0"
namespace x = "http://www.w3.org/1999/xhtml"


## when an element is not supported at the provider end, it must have
## an unsupported attribute with "true" value. Otherwise, it may explicitly 
## indicate support with a "false" value, but absence of this att is ok too.
imml.unsupported =
  attribute unsupported { "true" }
  & almostAnyContent
imml.supported = attribute unsupported { "false" }?
start = imml.root
imml.root |=
  imml.message.attributes
  & (imml.message.error | (imml.header?, imml.message.content?))
imml.message.attributes =
  attribute timestamp { xsd:dateTime }?
  & attribute message-id { imml.message.id.datatype }?
imml.message.error =
  element error {
    attribute value { imml.message.error.values }
    & empty
  }
# # # # #  # # # # # 
# Transport Header #
# # # #  # # # # # #
imml.header =
  element header {
    imml.header.params?
    & # Authentication is optional, but if present, should be associated with the actual reseller
      (imml.header.reseller & imml.header.authentication?)?
    & imml.header.reason?
    & imml.header.test?
  }
# External parameters for requests (instead of typical REST parameters)
imml.header.params =
  element params {
    element param {
      attribute name { imml.header.param.name.datatype }
      & attribute value { xsd:NMTOKEN }
    }+
  }
# Emitter or recipient can use immateriel reseller id or clil gencod
imml.header.reseller = element reseller { imml.header.reseller.id }
imml.header.reseller.id =
  attribute id { imml.reseller.id.datatype }
  | attribute dilicom_gencod { imml.reseller.clil_gencod.datatype }
# Make sure the emitter is the right one
imml.header.authentication =
  element authentication {
    attribute api_key { imml.authentication.key.datatype }
  }
# Sent by immateriel to tell (in human language) why the request was
# initiated. The type ("info" or "error") indicates if the request was made in
# the normal information exchange process or because of something unexpected
# happened.
imml.header.reason =
  element reason {
    attribute type { imml.header.reason.type.datatype }
    & text
  }
# For testing new ws urls
imml.header.test =
  element test {
    element receive { imml.header.url.attribute }?
    & element check { imml.header.url.attribute }?
    & element sales { imml.header.url.attribute }?
  }
imml.header.url.attribute =
  attribute url { imml.header.url.datatype }
  & empty
# End of Header #
# # # # # # # # #
imml.message.content = imml.book | imml.reporting

## Book ##
##
imml.book =
  element book {
    attribute ean { imml.offer.ean.datatype }
    & attribute uid { imml.book.id.datatype }?
    & imml.book.metadata
    & imml.book.assets
    & imml.offer
  }

##
# Book metadata
##
imml.book.metadata =
  element metadata {
    imml.book.metadata.title
    & imml.book.metadata.language
    & imml.book.metadata.subtitle?
    & imml.book.metadata.collection?
    & imml.book.metadata.pubdate?
    & imml.book.metadata.publisher
    & imml.book.metadata.contributors
    & imml.book.metadata.topics
    & imml.book.metadata.description
  }
imml.book.metadata.title.attributes = empty
imml.book.metadata.subtitle.attributes = empty
imml.book.metadata.contributors.attributes = empty
# Work title
imml.book.metadata.title =
  element title { imml.book.metadata.title.attributes, text }
# Work subtitle
imml.book.metadata.subtitle =
  element subtitle { imml.book.metadata.subtitle.attributes, text }
# First publication date for this work
imml.book.metadata.pubdate.attributes = empty
imml.book.metadata.pubdate =
  element publication {
    imml.book.metadata.pubdate.attributes, xsd:date
  }
imml.book.metadata.publisher.attributes =
  attribute uid { imml.book.metadata.publisher.id.datatype }?
# Book current publisher
imml.book.metadata.publisher =
  element publisher { imml.book.metadata.publisher.attributes, text }
# Book collection
imml.book.metadata.collection =
  element collection {
    attribute uid { imml.book.metadata.collection.id.datatype }?
    & text
  }
# Book language
imml.book.metadata.language =
  element language { imml.book.metadata.language.datatype }
  | attribute lang { imml.book.metadata.language.datatype }
# Book contributors
imml.book.metadata.contributors =
  element contributors {
    imml.book.metadata.contributors.attributes,
    imml.book.metadata.contributor+
  }
imml.book.metadata.contributor =
  element contributor {
    attribute uid { imml.book.contributor.id.datatype }?
    & imml.book.metadata.contributor.name
    & imml.book.metadata.contributor.role
  }
imml.book.metadata.contributor.name = element name { text }
imml.book.metadata.contributor.role =
  imml.book.metadata.contributor.role.supported
  | imml.book.metadata.contributor.role.unsupported
imml.book.metadata.contributor.role.supported =
  element role { imml.book.contributor.datatype & imml.supported }
imml.book.metadata.contributor.role.unsupported =
  element role { imml.unsupported }
# Book topics
imml.book.metadata.topics =
  imml.book.metadata.topics.supported
  | imml.book.metadata.topics.unsupported
imml.book.metadata.topics.supported =
  element topics { imml.book.metadata.topic+ & imml.supported }
imml.book.metadata.topics.unsupported =
  element topics { imml.unsupported }
imml.book.metadata.topic =
  element topic {
    imml.book.metadata.topic.bisac
    | imml.book.metadata.topic.clil
    | imml.book.metadata.topic.keyword
  }
imml.book.metadata.topic.bisac =
  imml.book.metadata.topic.bisac.supported
  | imml.book.metadata.topic.bisac.unsupported
imml.book.metadata.topic.bisac.supported =
  attribute type { "bisac" }
  & imml.supported
  & imml.bisac.id.datatype
imml.book.metadata.topic.bisac.unsupported =
  attribute type { "bisac" }
  & imml.unsupported
imml.book.metadata.topic.clil =
  imml.book.metadata.topic.clil.supported
  | imml.book.metadata.topic.clil.unsupported
imml.book.metadata.topic.clil.supported =
  attribute type { "clil" }
  & imml.supported
  & imml.clil.id.datatype
imml.book.metadata.topic.clil.unsupported =
  attribute type { "clil" }
  & imml.unsupported
imml.book.metadata.topic.keyword =
  imml.book.metadata.topic.keyword.supported
  | imml.book.metadata.topic.keyword.unsupported
imml.book.metadata.topic.keyword.supported =
  attribute type { "keyword" }
  & imml.supported
  & imml.keyword.id.datatype
imml.book.metadata.topic.keyword.unsupported =
  attribute type { "keyword" }
  & imml.unsupported
# Book description
imml.book.metadata.description.attributes =
  
  ## Default to book language (although ideally bookseller preferred language)
  attribute lang { imml.book.metadata.language.datatype }?
  & attribute extent { "short" | "long" | "140" }?
imml.book.metadata.description =
  element description {
    imml.book.metadata.description.content.xhtml
    | imml.book.metadata.description.content.text
  }
imml.book.metadata.description.content.xhtml =
  imml.book.metadata.description.xhtml.supported
  | imml.book.metadata.description.xhtml.unsupported
imml.book.metadata.description.xhtml.supported =
  imml.book.metadata.description.attributes
  & attribute format { "xhtml" }
  & imml.supported
  & anyXHTMLElement
imml.book.metadata.description.xhtml.unsupported =
  (imml.book.metadata.description.attributes,
   attribute format { "xhtml" })?
  & imml.unsupported
imml.book.metadata.description.content.text =
  imml.book.metadata.description.attributes, text

##
# Book assets
##
imml.book.assets =
  element assets {
    imml.book.assets.cover
    & imml.book.assets.extract*
    & imml.book.assets.full*
  }
imml.book.assets.mandatory_attr.common =
  attribute mimetype { imml.book.assets.mimetype.datatype }
imml.book.assets.optional_attr.common =
  attribute size {
    xsd:integer { minInclusive = "0" }
  }?
  & attribute url { imml.book.assets.url.datatype }?
  & attribute checksum {
      xsd:token { pattern = "\S*" }
    }?
  & attribute last_modified { xsd:dateTime }?

## Cover files are made available to the receiving reseller via an ad hoc url
imml.book.assets.cover =
  element cover {
    imml.book.assets.optional_attr.common
    & imml.book.assets.mandatory_attr.common
  }

## Extract files are made available to the receiving reseller via an ad hoc url
imml.book.assets.extract =
  element extract {
    imml.book.assets.extract.supported
    | imml.book.assets.extract.unsupported
  }
imml.book.assets.extract.supported =
  imml.book.assets.extract.content & imml.supported
imml.book.assets.extract.unsupported = imml.unsupported
imml.book.assets.extract.content =
  imml.book.assets.mandatory_attr.common
  & imml.book.assets.optional_attr.common

## Content files are made available via an ad hoc url tailored for the receiving warehouse
imml.book.assets.full =
  element full {
    imml.book.assets.full.supported | imml.book.assets.full.unsupported
  }
imml.book.assets.full.supported =
  imml.book.assets.full.content & imml.supported
imml.book.assets.full.unsupported = imml.unsupported
imml.book.assets.full.content =
  imml.book.assets.mandatory_attr.common
  & imml.book.assets.optional_attr.common

##
# Book offer
##
imml.offer =
  element offer {
    # =ex-support
    imml.offer.medium?
    & imml.offer.manifestation?
    & imml.offer.pagination?
    & imml.offer.alternatives?
    & # if not ready for sale, allow price to be missing
      (element ready_for_sale { "false" }
       | (element ready_for_sale { xsd:boolean }
          & imml.offer.prices))
    & imml.offer.sales_start_at
    & imml.offer.sales_models?
  }

## Various types of sales models
imml.offer.sales_models =
  imml.offer.sales_models.supported
  | imml.offer.sales_models.unsupported
imml.offer.sales_models.supported =
  element sales_models { imml.offer.sales_model+ & imml.supported }
imml.offer.sales_models.unsupported =
  element sales_models { imml.unsupported }
imml.offer.sales_model =
  element sales_model {
    attribute type { "purchase" | "subscription" | "freemium" }
    & attribute available { xsd:boolean }
    & attribute customer { "individual" | "library" }
    & attribute format { "epub" | "pdf" | "bundle" | "streaming" }
    & attribute protection { "none" | "watermark" | "drm" }
  }

## Book medium (for now, printed|digital)
imml.offer.medium = element medium { imml.offer.medium.datatype }

## Manifestation
imml.offer.manifestation =
  element format { imml.offer.manifestation.datatype }
imml.offer.pagination.attributes = empty

## Pagination of whatever alternative format where it makes sense
imml.offer.pagination =
  element pagination {
    imml.offer.pagination.attributes, imml.offer.pagination.datatype
  }

## Alternative offers for the same content
imml.offer.alternatives =
  element alternatives { imml.offer.alternative+ }
imml.offer.alternative =
  element alternative {
    attribute ean { imml.offer.ean.datatype }
    & attribute medium { imml.offer.medium.datatype }?
    & attribute format { imml.offer.manifestation.datatype }?
  }

## Date de mise à disposition
imml.offer.sales_start_at =
  imml.offer.sales_start_at.supported
  | imml.offer.sales_start_at.unsupported
imml.offer.sales_start_at.supported =
  element sales_start_at { imml.supported & xsd:date }
imml.offer.sales_start_at.unsupported =
  element sales_start_at { imml.unsupported }
imml.offer.prices.attributes = empty

## Book prices
imml.offer.prices =
  element prices { imml.offer.prices.attributes, imml.offer.price+ }
imml.offer.price =
  element price {
    imml.offer.price.currency
    & imml.offer.price.current_amount
    & imml.offer.price.territories
    & imml.offer.price.intervals?
  }
# Currency
imml.offer.price.currency =
  attribute currency { imml.offer.price.currency.datatype }
# Public price amount
imml.offer.price.current_amount =
  element current_amount { imml.offer.price.datatype }

## Book territories
imml.offer.price.territories =
  element territories { imml.offer.territory.datatype }
imml.offer.price.intervals =
  imml.offer.price.intervals.supported
  | imml.offer.price.intervals.unsupported
imml.offer.price.intervals.supported =
  element intervals { imml.supported & imml.offer.price.interval+ }
imml.offer.price.intervals.unsupported =
  element intervals { imml.unsupported }
imml.offer.price.interval =
  element interval {
    attribute start_at { xsd:date }?
    & attribute end_at { xsd:date }?
    & imml.offer.price.datatype
  }

## Reporting ##
##
imml.reporting =
  element reporting {
    attribute date { xsd:date }
    & element lines { imml.order.line+ }
  }
imml.order.line =
  element line {
    attribute ean { imml.offer.ean.datatype }
    & attribute type { imml.offer.type.datatype }
    & attribute customer { imml.offer.customer.datatype }
    & attribute net_amount {
        xsd:decimal { fractionDigits = "2" }
      }
    & attribute quantity { xsd:integer }
    & attribute tax {
        xsd:decimal { minInclusive = "0" fractionDigits = "1" }
      }
    & attribute unit_price {
        xsd:decimal { minInclusive = "0" fractionDigits = "2" }
      }
    & attribute currency { imml.offer.price.currency.datatype }
    & attribute country { imml.offer.territory.datatype }
    & imml.order.line.xtra?
  }
imml.order.line.xtra =
  element extra_param {
    attribute key { xsd:token }
    & attribute value { text }
  }

## Tools for expressing "whatever child"
## See http://www.relaxng.org/compact-tutorial-20030326.html#id2816343
anyElement =
  element * {
    (attribute * { text }
     | text
     | anyElement)*
  }
anyXHTMLElement =
  element x:* {
    (attribute * { text }
     | text
     | anyXHTMLElement)*
  }
# So not exactly "any" content. Part of the not-quite-clean handling of "unsupported" features
almostAnyContent =
  (attribute * - (unsupported | type | format) { text }
   | text
   | anyElement)*

## Datatypes ##
##
imml.message.error.values =
  "bad_territory" | "out_of_tier" | "client_already_exists"
imml.header.param.name.datatype = xsd:token
imml.book.assets.url.datatype = xsd:anyURI
imml.book.assets.mimetype.datatype =
  "image/jpeg"
  | "image/png"
  | "audio/mpeg"
  | "video/mpeg"
  | "video/mp4"
  | "video/x-flv"
  | "application/pdf"
  | "text/html"
  | "application/zip"
  | "application/x-mobipocket"
  | "application/x-mobipocket-ebook"
  | "application/x-rar-compressed"
  | "application/epub+zip"
imml.book.contributor.datatype =
  list { "author" | "translator" | "editor" | "typesetter" }
imml.bisac.id.datatype = xsd:token { pattern = "[A-Z]{3}[0-9]{6}" }
imml.clil.id.datatype = xsd:token { pattern = "[0-9]{4}" }
imml.keyword.id.datatype = text
imml.offer.type.datatype = "purchase" | "subscription" | "freemium"
imml.offer.customer.datatype = "individual" | "library"
imml.offer.medium.datatype = "printed" | "digital"
imml.offer.manifestation.datatype =
  "epub" | "pdf" | "streaming" | "bundle"
imml.offer.pagination.datatype = xsd:integer { minInclusive = "0" }
imml.offer.price.datatype =
  xsd:decimal { minInclusive = "0" fractionDigits = "2" }
imml.book.metadata.language.datatype = list { List74+ }
imml.message.id.datatype = xsd:NMTOKEN
imml.header.reason.type.datatype = "info" | "error"
imml.header.url.datatype = xsd:anyURI
imml.reseller.id.datatype = xsd:NMTOKEN
# Diffuseurs/distributeurs commencent par 301, libraires par 302, éditeurs par 305.
imml.reseller.clil_gencod.datatype =
  xsd:token { pattern = "302[0-9]{10}" }
imml.authentication.key.datatype = xsd:NMTOKEN
imml.uid.datatype = xsd:token
imml.book.id.datatype = xsd:token
imml.book.metadata.publisher.id.datatype = xsd:token
imml.book.metadata.collection.id.datatype = xsd:token
imml.book.contributor.id.datatype = imml.uid.datatype
imml.offer.ean.datatype =
  xsd:token { length = "13" pattern = "3[0-9]{12}|97[8-9][0-9]{10}" }
imml.offer.territory.datatype = "WORLD" | imml.offer.country.datatype
imml.offer.country.datatype = list { List91+ }

