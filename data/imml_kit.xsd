  <xs:element name="alternative">
    <xs:complexType>
      <xs:attribute name="ean" use="required" type="imml.offer.ean.datatype"/>
      <xs:attribute name="medium" type="imml.offer.medium.datatype"/>
      <xs:attribute name="format" type="imml.offer.manifestation.datatype"/>
    </xs:complexType>
  </xs:element>
  <xs:group name="imml.offer.sales_start_at">
    <xs:annotation>
      <xs:documentation>Date de mise à disposition</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:group ref="imml.offer.sales_start_at.supported"/>
      <xs:group ref="imml.offer.sales_start_at.unsupported"/>
    </xs:choice>
  </xs:group>
  <xs:group name="imml.offer.sales_start_at.supported">
    <xs:sequence>
      <xs:element name="sales_start_at">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:date">
              <xs:attributeGroup ref="imml.supported"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="imml.offer.sales_start_at.unsupported">
    <xs:sequence>
      <xs:element name="sales_start_at">
        <xs:complexType mixed="true">
          <xs:group ref="imml.unsupported"/>
          <xs:attributeGroup ref="imml.unsupported"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="prices">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="price"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="price">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="current_amount"/>
        <xs:element ref="territories"/>
        <xs:group ref="imml.offer.price.intervals"/>
      </xs:choice>
      <xs:attributeGroup ref="imml.offer.price.currency"/>
    </xs:complexType>
  </xs:element>
  <!-- Currency -->
  <xs:attributeGroup name="imml.offer.price.currency">
    <xs:attribute name="currency" use="required" type="imml.offer.price.currency.datatype"/>
  </xs:attributeGroup>
  <!-- Public price amount -->
  <xs:element name="current_amount" type="imml.offer.price.datatype"/>
  <xs:element name="territories" type="imml.offer.territory.datatype"/>
  <xs:group name="imml.offer.price.intervals">
    <xs:choice>
      <xs:group ref="imml.offer.price.intervals.supported"/>
      <xs:group ref="imml.offer.price.intervals.unsupported"/>
    </xs:choice>
  </xs:group>
  <xs:group name="imml.offer.price.intervals.supported">
    <xs:sequence>
      <xs:element name="intervals">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="interval"/>
          </xs:sequence>
          <xs:attributeGroup ref="imml.supported"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="imml.offer.price.intervals.unsupported">
    <xs:sequence>
      <xs:element name="intervals">
        <xs:complexType mixed="true">
          <xs:group ref="imml.unsupported"/>
          <xs:attributeGroup ref="imml.unsupported"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="interval">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="imml.offer.price.datatype">
          <xs:attribute name="start_at" type="xs:date"/>
          <xs:attribute name="end_at" type="xs:date"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="reporting" substitutionGroup="imml.message.content">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="lines"/>
      </xs:sequence>
      <xs:attribute name="date" use="required" type="xs:date"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="lines">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="line"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="line">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="extra_param"/>
      </xs:sequence>
      <xs:attribute name="ean" use="required" type="imml.offer.ean.datatype"/>
      <xs:attribute name="type" use="required" type="imml.offer.type.datatype"/>
      <xs:attribute name="customer" use="required" type="imml.offer.customer.datatype"/>
      <xs:attribute name="net_amount" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="2"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="quantity" use="required" type="xs:integer"/>
      <xs:attribute name="tax" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0"/>
            <xs:fractionDigits value="1"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="unit_price" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0"/>
            <xs:fractionDigits value="2"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="currency" use="required" type="imml.offer.price.currency.datatype"/>
      <xs:attribute name="country" use="required" type="imml.offer.territory.datatype"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="extra_param">
    <xs:complexType>
      <xs:attribute name="key" use="required" type="xs:token"/>
      <xs:attribute name="value" use="required"/>
    </xs:complexType>
  </xs:element>
