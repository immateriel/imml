  <xs:group name="imml.offer.sales_models">
    <xs:annotation>
      <xs:documentation>Various types of sales models</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:group ref="imml.offer.sales_models.supported"/>
      <xs:group ref="imml.offer.sales_models.unsupported"/>
    </xs:choice>
  </xs:group>
  <xs:group name="imml.offer.sales_models.supported">
    <xs:sequence>
      <xs:element name="sales_models">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="sales_model"/>
          </xs:sequence>
          <xs:attributeGroup ref="imml.supported"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="imml.offer.sales_models.unsupported">
    <xs:sequence>
      <xs:element name="sales_models">
        <xs:complexType mixed="true">
          <xs:group ref="imml.unsupported"/>
          <xs:attributeGroup ref="imml.unsupported"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="sales_model">
    <xs:complexType>
      <xs:attribute name="type" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="purchase"/>
            <xs:enumeration value="subscription"/>
            <xs:enumeration value="freemium"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="available" use="required" type="xs:boolean"/>
      <xs:attribute name="customer" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="individual"/>
            <xs:enumeration value="library"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="format" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="epub"/>
            <xs:enumeration value="pdf"/>
            <xs:enumeration value="bundle"/>
            <xs:enumeration value="streaming"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="protection" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="none"/>
            <xs:enumeration value="watermark"/>
            <xs:enumeration value="drm"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="medium" type="imml.offer.medium.datatype"/>
  <xs:element name="format" type="imml.offer.manifestation.datatype"/>
  <xs:element name="pagination" type="imml.offer.pagination.datatype"/>
  <xs:element name="alternatives">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="alternative"/>
      </xs:sequence>
    </xs:complexType>
