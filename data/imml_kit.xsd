<?xml version="1.0" encoding="UTF-8"?>
<!--
  # # # # # # # # # # # # #
  IMML schema definitions #
  IMML is the immatériel.fr preferred exchange format for metadata #
  
  *Unsupported elements* : when an element is not supported at the emitter end, it must
  contain an attribute "unsupported" with value "true". Otherwise, it may explicitly 
  indicate support with a "false" value, but absence of this att is ok too.
-->
<!-- default namespace = "http://ns.immateriel.fr/imml" -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:group name="imml.unsupported">
    <xs:annotation>
      <xs:documentation>when an element is not supported at the provider end, it must have
an unsupported attribute with "true" value. Otherwise, it may explicitly 
indicate support with a "false" value, but absence of this att is ok too.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:group ref="almostAnyContent"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="imml.unsupported">
    <xs:annotation>
      <xs:documentation>when an element is not supported at the provider end, it must have
an unsupported attribute with "true" value. Otherwise, it may explicitly 
indicate support with a "false" value, but absence of this att is ok too.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="unsupported" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="true"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="almostAnyContent"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="imml.supported">
    <xs:attribute name="unsupported">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="false"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="imml">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="error"/>
        <xs:sequence>
          <xs:element minOccurs="0" ref="header"/>
          <xs:element minOccurs="0" ref="imml.message.content"/>
        </xs:sequence>
      </xs:choice>
      <xs:attributeGroup ref="imml.message.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="imml.message.attributes">
    <xs:attribute name="version" use="required" type="imml.schema.version"/>
    <xs:attribute name="timestamp" type="xs:dateTime"/>
    <xs:attribute name="message-id" type="imml.message.id.datatype"/>
  </xs:attributeGroup>
  <xs:element name="error">
    <xs:complexType>
      <xs:attribute name="value" use="required" type="imml.message.error.values"/>
    </xs:complexType>
  </xs:element>
  <!--
    # # # #  # # # # # 
    Transport Header #
    # # #  # # # # # #
  -->
  <xs:element name="header">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="params"/>
        <xs:choice>
          <xs:element ref="reseller"/>
          <xs:element ref="authentication"/>
        </xs:choice>
        <xs:element ref="reason"/>
        <xs:element ref="test"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!-- External parameters for requests (instead of typical REST parameters) -->
  <xs:element name="params">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="param"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="param">
    <xs:complexType>
      <xs:attribute name="name" use="required" type="imml.header.param.name.datatype"/>
      <xs:attribute name="value" use="required" type="xs:NMTOKEN"/>
    </xs:complexType>
  </xs:element>
  <!-- Emitter or recipient can use immateriel reseller id or clil gencod -->
  <xs:element name="reseller">
    <xs:complexType>
      <xs:attributeGroup ref="imml.header.reseller.id"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="imml.header.reseller.id">
    <xs:attribute name="id" type="imml.reseller.id.datatype"/>
    <xs:attribute name="dilicom_gencod" type="imml.reseller.clil_gencod.datatype"/>
  </xs:attributeGroup>
  <!-- Make sure the emitter is the right one -->
  <xs:element name="authentication">
    <xs:complexType>
      <xs:attribute name="api_key" use="required" type="imml.authentication.key.datatype"/>
    </xs:complexType>
  </xs:element>
  <!--
    Sent by immateriel to tell (in human language) why the request was
    initiated. The type ("info" or "error") indicates if the request was made in
    the normal information exchange process or because of something unexpected
    happened.
  -->
  <xs:element name="reason">
    <xs:complexType mixed="true">
      <xs:attribute name="type" use="required" type="imml.header.reason.type.datatype"/>
    </xs:complexType>
  </xs:element>
  <!-- For testing new ws urls -->
  <xs:element name="test">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" ref="receive"/>
        <xs:element minOccurs="0" ref="check"/>
        <xs:element minOccurs="0" ref="sales"/>
      </xs:all>
    </xs:complexType>
  </xs:element>
  <xs:element name="receive">
    <xs:complexType>
      <xs:attributeGroup ref="imml.header.url.attribute"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="check">
    <xs:complexType>
      <xs:attributeGroup ref="imml.header.url.attribute"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="sales">
    <xs:complexType>
      <xs:attributeGroup ref="imml.header.url.attribute"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="imml.header.url.attribute">
    <xs:attribute name="url" use="required" type="imml.header.url.datatype"/>
  </xs:attributeGroup>
  <!--
    End of Header #
    # # # # # # # #
  -->
  <xs:element name="imml.message.content" abstract="true"/>
  <xs:element name="book" substitutionGroup="imml.message.content">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="metadata"/>
        <xs:element ref="assets"/>
        <xs:element ref="offer"/>
      </xs:choice>
      <xs:attribute name="ean" use="required" type="imml.offer.ean.datatype"/>
      <xs:attribute name="uid" type="imml.book.id.datatype"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="metadata">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="title"/>
        <xs:group ref="imml.book.metadata.language"/>
        <xs:element ref="subtitle"/>
        <xs:element ref="collection"/>
        <xs:element ref="publication"/>
        <xs:element ref="publisher"/>
        <xs:element ref="contributors"/>
        <xs:group ref="imml.book.metadata.topics"/>
        <xs:element ref="description"/>
      </xs:choice>
      <xs:attributeGroup ref="imml.book.metadata.language"/>
    </xs:complexType>
  </xs:element>
  <!-- Work title -->
  <xs:element name="title" type="xs:string"/>
  <!-- Work subtitle -->
  <xs:element name="subtitle" type="xs:string"/>
  <!-- First publication date for this work -->
  <xs:element name="publication" type="xs:date"/>
  <!-- Book current publisher -->
  <xs:element name="publisher">
    <xs:complexType mixed="true">
      <xs:attribute name="uid" type="imml.book.metadata.publisher.id.datatype"/>
    </xs:complexType>
  </xs:element>
  <!-- Book collection -->
  <xs:element name="collection">
    <xs:complexType mixed="true">
      <xs:attribute name="uid" type="imml.book.metadata.collection.id.datatype"/>
    </xs:complexType>
  </xs:element>
  <!-- Book language -->
  <xs:group name="imml.book.metadata.language">
    <xs:sequence>
      <xs:element minOccurs="0" ref="language"/>
    </xs:sequence>
  </xs:group>
  <xs:element name="language" type="imml.book.metadata.language.datatype"/>
  <xs:attributeGroup name="imml.book.metadata.language">
    <xs:attribute name="lang" type="imml.book.metadata.language.datatype"/>
  </xs:attributeGroup>
  <!-- Book contributors -->
  <xs:element name="contributors">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="contributor"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="contributor">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="name"/>
        <xs:group ref="imml.book.metadata.contributor.role"/>
      </xs:choice>
      <xs:attribute name="uid" type="imml.book.contributor.id.datatype"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="name" type="xs:string"/>
  <xs:group name="imml.book.metadata.contributor.role">
    <xs:choice>
      <xs:group ref="imml.book.metadata.contributor.role.supported"/>
      <xs:group ref="imml.book.metadata.contributor.role.unsupported"/>
    </xs:choice>
  </xs:group>
  <xs:group name="imml.book.metadata.contributor.role.supported">
    <xs:sequence>
      <xs:element name="role">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="imml.book.contributor.datatype">
              <xs:attributeGroup ref="imml.supported"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="imml.book.metadata.contributor.role.unsupported">
    <xs:sequence>
      <xs:element name="role">
        <xs:complexType mixed="true">
          <xs:group ref="imml.unsupported"/>
          <xs:attributeGroup ref="imml.unsupported"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <!-- Book topics -->
  <xs:group name="imml.book.metadata.topics">
    <xs:choice>
      <xs:group ref="imml.book.metadata.topics.supported"/>
      <xs:group ref="imml.book.metadata.topics.unsupported"/>
    </xs:choice>
  </xs:group>
  <xs:group name="imml.book.metadata.topics.supported">
    <xs:sequence>
      <xs:element name="topics">
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="topic"/>
          </xs:sequence>
          <xs:attributeGroup ref="imml.supported"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:group name="imml.book.metadata.topics.unsupported">
    <xs:sequence>
      <xs:element name="topics">
        <xs:complexType mixed="true">
          <xs:group ref="imml.unsupported"/>
          <xs:attributeGroup ref="imml.unsupported"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:element name="topic">
    <xs:complexType mixed="true">
      <xs:choice>
        <xs:group ref="imml.book.metadata.topic.bisac"/>
        <xs:group ref="imml.book.metadata.topic.clil"/>
        <xs:group ref="imml.book.metadata.topic.keyword"/>
      </xs:choice>
      <xs:attribute name="type" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="bisac"/>
            <xs:enumeration value="clil"/>
            <xs:enumeration value="keyword"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="unsupported">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="false"/>
            <xs:enumeration value="true"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attributeGroup ref="almostAnyContent"/>
    </xs:complexType>
  </xs:element>
  <xs:group name="imml.book.metadata.topic.bisac">
    <xs:sequence>
      <xs:group minOccurs="0" ref="imml.book.metadata.topic.bisac.unsupported"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="imml.book.metadata.topic.bisac">
    <xs:attribute name="unsupported">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="false"/>
          <xs:enumeration value="true"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="bisac"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="almostAnyContent"/>
  </xs:attributeGroup>
  <xs:simpleType name="imml.book.metadata.topic.bisac.supported">
    <xs:restriction base="imml.bisac.id.datatype"/>
  </xs:simpleType>
  <xs:attributeGroup name="imml.book.metadata.topic.bisac.supported">
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="bisac"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="imml.supported"/>
  </xs:attributeGroup>
  <xs:group name="imml.book.metadata.topic.bisac.unsupported">
    <xs:sequence>
      <xs:group ref="imml.unsupported"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="imml.book.metadata.topic.bisac.unsupported">
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="bisac"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="imml.unsupported"/>
  </xs:attributeGroup>
  <xs:group name="imml.book.metadata.topic.clil">
    <xs:sequence>
      <xs:group minOccurs="0" ref="imml.book.metadata.topic.clil.unsupported"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="imml.book.metadata.topic.clil">
    <xs:attribute name="unsupported">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="false"/>
          <xs:enumeration value="true"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="clil"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="almostAnyContent"/>
  </xs:attributeGroup>
  <xs:simpleType name="imml.book.metadata.topic.clil.supported">
    <xs:restriction base="imml.clil.id.datatype"/>
  </xs:simpleType>
  <xs:attributeGroup name="imml.book.metadata.topic.clil.supported">
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="clil"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="imml.supported"/>
  </xs:attributeGroup>
  <xs:group name="imml.book.metadata.topic.clil.unsupported">
    <xs:sequence>
      <xs:group ref="imml.unsupported"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="imml.book.metadata.topic.clil.unsupported">
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="clil"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="imml.unsupported"/>
  </xs:attributeGroup>
  <xs:group name="imml.book.metadata.topic.keyword">
    <xs:sequence>
      <xs:group minOccurs="0" ref="imml.book.metadata.topic.keyword.unsupported"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="imml.book.metadata.topic.keyword">
    <xs:attribute name="unsupported">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="false"/>
          <xs:enumeration value="true"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="keyword"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="almostAnyContent"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="imml.book.metadata.topic.keyword.supported">
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="keyword"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="imml.supported"/>
  </xs:attributeGroup>
  <xs:group name="imml.book.metadata.topic.keyword.unsupported">
    <xs:sequence>
      <xs:group ref="imml.unsupported"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="imml.book.metadata.topic.keyword.unsupported">
    <xs:attribute name="type" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="keyword"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="imml.unsupported"/>
  </xs:attributeGroup>
  <!-- Book description -->
  <xs:attributeGroup name="imml.book.metadata.description.attributes">
    <xs:attribute name="lang" type="imml.book.metadata.language.datatype">
      <xs:annotation>
        <xs:documentation>Default to book language (although ideally bookseller preferred language)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="extent">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="short"/>
          <xs:enumeration value="long"/>
          <xs:enumeration value="140"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="description">
    <xs:complexType mixed="true">
      <xs:group minOccurs="0" ref="imml.book.metadata.description.content.xhtml"/>
      <xs:attributeGroup ref="imml.book.metadata.description.content.xhtml"/>
    </xs:complexType>
  </xs:element>
  <xs:group name="imml.book.metadata.description.content.xhtml">
    <xs:choice>
      <xs:group ref="imml.book.metadata.description.xhtml.supported"/>
      <xs:group ref="imml.book.metadata.description.xhtml.unsupported"/>
    </xs:choice>
  </xs:group>
  <xs:attributeGroup name="imml.book.metadata.description.content.xhtml">
    <xs:attribute name="unsupported">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="false"/>
          <xs:enumeration value="true"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="format">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="xhtml"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="almostAnyContent"/>
  </xs:attributeGroup>
  <xs:group name="imml.book.metadata.description.xhtml.supported">
    <xs:sequence>
      <xs:group ref="anyXHTMLElement"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="imml.book.metadata.description.xhtml.supported">
    <xs:attribute name="format" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="xhtml"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="imml.supported"/>
  </xs:attributeGroup>
  <xs:group name="imml.book.metadata.description.xhtml.unsupported">
    <xs:sequence>
      <xs:group ref="imml.unsupported"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="imml.book.metadata.description.xhtml.unsupported">
    <xs:attribute name="format">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="xhtml"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attributeGroup ref="imml.unsupported"/>
  </xs:attributeGroup>
  <xs:element name="assets">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="cover"/>
        <xs:element ref="extract"/>
        <xs:element ref="full"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="imml.book.assets.mandatory_attr.common">
    <xs:attribute name="mimetype" use="required" type="imml.book.assets.mimetype.datatype"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="imml.book.assets.optional_attr.common">
    <xs:attribute name="size">
      <xs:simpleType>
        <xs:restriction base="xs:integer">
          <xs:minInclusive value="0"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="url" type="imml.book.assets.url.datatype"/>
    <xs:attribute name="checksum">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:pattern value="\S*"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="last_modified" type="xs:dateTime"/>
  </xs:attributeGroup>
  <xs:element name="cover">
    <xs:complexType>
      <xs:attributeGroup ref="imml.book.assets.optional_attr.common"/>
      <xs:attributeGroup ref="imml.book.assets.mandatory_attr.common"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="extract">
    <xs:complexType mixed="true">
      <xs:group minOccurs="0" ref="imml.book.assets.extract.unsupported"/>
      <xs:attribute name="unsupported">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="false"/>
            <xs:enumeration value="true"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attributeGroup ref="almostAnyContent"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="imml.book.assets.extract.supported">
    <xs:attributeGroup ref="imml.book.assets.extract.content"/>
    <xs:attributeGroup ref="imml.supported"/>
  </xs:attributeGroup>
  <xs:group name="imml.book.assets.extract.unsupported">
    <xs:sequence>
      <xs:group ref="imml.unsupported"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="imml.book.assets.extract.unsupported">
    <xs:attributeGroup ref="imml.unsupported"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="imml.book.assets.extract.content">
    <xs:attributeGroup ref="imml.book.assets.mandatory_attr.common"/>
    <xs:attributeGroup ref="imml.book.assets.optional_attr.common"/>
  </xs:attributeGroup>
  <xs:element name="full">
    <xs:complexType mixed="true">
      <xs:group minOccurs="0" ref="imml.book.assets.full.unsupported"/>
      <xs:attribute name="unsupported">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="false"/>
            <xs:enumeration value="true"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attributeGroup ref="almostAnyContent"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="imml.book.assets.full.supported">
    <xs:attributeGroup ref="imml.book.assets.full.content"/>
    <xs:attributeGroup ref="imml.supported"/>
  </xs:attributeGroup>
  <xs:group name="imml.book.assets.full.unsupported">
    <xs:sequence>
      <xs:group ref="imml.unsupported"/>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="imml.book.assets.full.unsupported">
    <xs:attributeGroup ref="imml.unsupported"/>
  </xs:attributeGroup>
  <xs:attributeGroup name="imml.book.assets.full.content">
    <xs:attributeGroup ref="imml.book.assets.mandatory_attr.common"/>
    <xs:attributeGroup ref="imml.book.assets.optional_attr.common"/>
  </xs:attributeGroup>
  <xs:element name="offer">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="medium"/>
        <xs:element ref="format"/>
        <xs:element ref="pagination"/>
        <xs:element ref="alternatives"/>
        <xs:choice>
          <xs:element name="ready_for_sale">
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="false"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:choice>
            <xs:element name="ready_for_sale" type="xs:boolean"/>
            <xs:element ref="prices"/>
          </xs:choice>
        </xs:choice>
        <xs:group ref="imml.offer.sales_start_at"/>
        <xs:group ref="imml.offer.sales_models"/>
      </xs:choice>
    </xs:complexType>
