    <element name="sales_start_at">
      <ref name="imml.unsupported"/>
    </element>
  </define>
  <define name="imml.offer.prices.attributes">
    <empty/>
  </define>
  <define name="imml.offer.prices">
    <a:documentation>Book prices</a:documentation>
    <element name="prices">
      <ref name="imml.offer.prices.attributes"/>
      <oneOrMore>
        <ref name="imml.offer.price"/>
      </oneOrMore>
    </element>
  </define>
  <define name="imml.offer.price">
    <element name="price">
      <interleave>
        <ref name="imml.offer.price.currency"/>
        <ref name="imml.offer.price.current_amount"/>
        <ref name="imml.offer.price.territories"/>
        <optional>
          <ref name="imml.offer.price.intervals"/>
        </optional>
      </interleave>
    </element>
  </define>
  <!-- Currency -->
  <define name="imml.offer.price.currency">
    <attribute name="currency">
      <ref name="imml.offer.price.currency.datatype"/>
    </attribute>
  </define>
  <!-- Public price amount -->
  <define name="imml.offer.price.current_amount">
    <element name="current_amount">
      <ref name="imml.offer.price.datatype"/>
    </element>
  </define>
  <define name="imml.offer.price.territories">
    <a:documentation>Book territories</a:documentation>
    <element name="territories">
      <ref name="imml.offer.territory.datatype"/>
    </element>
  </define>
  <define name="imml.offer.price.intervals">
    <choice>
      <ref name="imml.offer.price.intervals.supported"/>
      <ref name="imml.offer.price.intervals.unsupported"/>
    </choice>
  </define>
  <define name="imml.offer.price.intervals.supported">
    <element name="intervals">
      <interleave>
        <ref name="imml.supported"/>
        <oneOrMore>
          <ref name="imml.offer.price.interval"/>
        </oneOrMore>
      </interleave>
    </element>
  </define>
  <define name="imml.offer.price.intervals.unsupported">
    <element name="intervals">
      <ref name="imml.unsupported"/>
    </element>
  </define>
  <define name="imml.offer.price.interval">
    <element name="interval">
      <interleave>
        <optional>
          <attribute name="start_at">
            <data type="date"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="end_at">
            <data type="date"/>
          </attribute>
        </optional>
        <ref name="imml.offer.price.datatype"/>
      </interleave>
    </element>
  </define>
  <define name="imml.reporting">
    <a:documentation>Reporting ##
</a:documentation>
    <element name="reporting">
      <interleave>
        <attribute name="date">
          <data type="date"/>
        </attribute>
        <element name="lines">
          <oneOrMore>
            <ref name="imml.order.line"/>
          </oneOrMore>
        </element>
      </interleave>
    </element>
  </define>
  <define name="imml.order.line">
    <element name="line">
      <interleave>
        <attribute name="ean">
          <ref name="imml.offer.ean.datatype"/>
        </attribute>
        <attribute name="type">
          <ref name="imml.offer.type.datatype"/>
        </attribute>
        <attribute name="customer">
          <ref name="imml.offer.customer.datatype"/>
        </attribute>
        <attribute name="net_amount">
          <data type="decimal">
            <param name="fractionDigits">2</param>
          </data>
        </attribute>
        <attribute name="quantity">
          <data type="integer"/>
        </attribute>
        <attribute name="tax">
          <data type="decimal">
            <param name="minInclusive">0</param>
            <param name="fractionDigits">1</param>
          </data>
        </attribute>
        <attribute name="unit_price">
          <data type="decimal">
            <param name="minInclusive">0</param>
            <param name="fractionDigits">2</param>
          </data>
        </attribute>
        <attribute name="currency">
          <ref name="imml.offer.price.currency.datatype"/>
        </attribute>
        <attribute name="country">
          <ref name="imml.offer.territory.datatype"/>
        </attribute>
        <optional>
          <ref name="imml.order.line.xtra"/>
        </optional>
      </interleave>
    </element>
  </define>
  <define name="imml.order.line.xtra">
    <element name="extra_param">
      <interleave>
        <attribute name="key">
          <data type="token"/>
        </attribute>
        <attribute name="value"/>
      </interleave>
    </element>
  </define>
  <define name="anyElement">
    <a:documentation>Tools for expressing &quot;whatever child&quot;
See http://www.relaxng.org/compact-tutorial-20030326.html#id2816343</a:documentation>
    <element>
      <anyName/>
      <zeroOrMore>
        <choice>
          <attribute>
            <anyName/>
          </attribute>
          <text/>
          <ref name="anyElement"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="anyXHTMLElement">
    <element>
      <nsName ns="http://www.w3.org/1999/xhtml"/>
      <zeroOrMore>
        <choice>
          <attribute>
            <anyName/>
          </attribute>
          <text/>
          <ref name="anyXHTMLElement"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <!-- So not exactly "any" content. Part of the not-quite-clean handling of "unsupported" features -->
  <define name="almostAnyContent">
    <zeroOrMore>
      <choice>
        <attribute>
          <anyName>
            <except>
              <name ns="">unsupported</name>
              <name ns="">type</name>
              <name ns="">format</name>
            </except>
          </anyName>
        </attribute>
        <text/>
        <ref name="anyElement"/>
      </choice>
    </zeroOrMore>
  </define>
