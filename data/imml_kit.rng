<?xml version="1.0" encoding="UTF-8"?>
<!--
  # # # # # # # # # # # # #
  IMML schema definitions #
  IMML is the immatÃ©riel.fr preferred exchange format for metadata #
  
  *Unsupported elements* : when an element is not supported at the emitter end, it must
  contain an attribute "unsupported" with value "true". Otherwise, it may explicitly 
  indicate support with a "false" value, but absence of this att is ok too.
-->
<!-- default namespace = "http://ns.immateriel.fr/imml" -->
<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" xmlns="http://relaxng.org/ns/structure/1.0" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns:x="http://www.w3.org/1999/xhtml">
  <define name="imml.unsupported">
    <a:documentation>when an element is not supported at the provider end, it must have
an unsupported attribute with &quot;true&quot; value. Otherwise, it may explicitly
indicate support with a &quot;false&quot; value, but absence of this att is ok too.</a:documentation>
    <interleave>
      <attribute name="unsupported">
        <value>true</value>
      </attribute>
      <ref name="almostAnyContent"/>
    </interleave>
  </define>
  <define name="imml.supported">
    <optional>
      <attribute name="unsupported">
        <value>false</value>
      </attribute>
    </optional>
  </define>
  <start>
    <ref name="imml.root"/>
  </start>
  <define combine="choice" name="imml.root">
    <interleave>
      <ref name="imml.message.attributes"/>
      <choice>
        <ref name="imml.message.error"/>
        <group>
          <optional>
            <ref name="imml.header"/>
          </optional>
          <optional>
            <ref name="imml.message.content"/>
          </optional>
        </group>
      </choice>
    </interleave>
  </define>
  <define name="imml.message.attributes">
    <interleave>
      <optional>
        <attribute name="timestamp">
          <data type="dateTime"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="message-id">
          <ref name="imml.message.id.datatype"/>
        </attribute>
      </optional>
    </interleave>
  </define>
  <define name="imml.message.error">
    <element name="error">
      <interleave>
        <attribute name="value">
          <ref name="imml.message.error.values"/>
        </attribute>
        <empty/>
      </interleave>
    </element>
  </define>
  <!--
    # # # #  # # # # # 
    Transport Header #
    # # #  # # # # # #
  -->
  <define name="imml.header">
    <element name="header">
      <interleave>
        <optional>
          <ref name="imml.header.params"/>
        </optional>
        <optional>
          <!-- Authentication is optional, but if present, should be associated with the actual reseller -->
          <interleave>
            <ref name="imml.header.reseller"/>
            <optional>
              <ref name="imml.header.authentication"/>
            </optional>
          </interleave>
        </optional>
        <optional>
          <ref name="imml.header.reason"/>
        </optional>
        <optional>
          <ref name="imml.header.test"/>
        </optional>
      </interleave>
    </element>
  </define>
  <!-- External parameters for requests (instead of typical REST parameters) -->
  <define name="imml.header.params">
    <element name="params">
      <oneOrMore>
        <element name="param">
          <interleave>
            <attribute name="name">
              <ref name="imml.header.param.name.datatype"/>
            </attribute>
            <attribute name="value">
              <data type="NMTOKEN"/>
            </attribute>
          </interleave>
        </element>
      </oneOrMore>
    </element>
  </define>
  <!-- Emitter or recipient can use immateriel reseller id or clil gencod -->
  <define name="imml.header.reseller">
    <element name="reseller">
      <ref name="imml.header.reseller.id"/>
    </element>
  </define>
  <define name="imml.header.reseller.id">
    <choice>
      <attribute name="reseller_id">
        <ref name="imml.reseller.id.datatype"/>
      </attribute>
      <attribute name="reseller_dilicom_gencod">
        <ref name="imml.reseller.clil_gencod.datatype"/>
      </attribute>
    </choice>
  </define>
  <!-- Make sure the emitter is the right one -->
  <define name="imml.header.authentication">
    <element name="authentication">
      <attribute name="api_key">
        <ref name="imml.authentication.key.datatype"/>
      </attribute>
    </element>
  </define>
  <!--
    Sent by immateriel to tell (in human language) why the request was
    initiated. The type ("info" or "error") indicates if the request was made in
    the normal information exchange process or because of something unexpected
    happened.
  -->
  <define name="imml.header.reason">
    <element name="reason">
      <interleave>
        <attribute name="type">
          <ref name="imml.header.reason.type.datatype"/>
        </attribute>
        <text/>
      </interleave>
    </element>
  </define>
  <!-- For testing new ws urls -->
  <define name="imml.header.test">
    <element name="test">
      <interleave>
        <optional>
          <element name="receive">
            <ref name="imml.header.url.attribute"/>
          </element>
        </optional>
        <optional>
          <element name="check">
            <ref name="imml.header.url.attribute"/>
          </element>
        </optional>
        <optional>
          <element name="sales">
            <ref name="imml.header.url.attribute"/>
          </element>
        </optional>
      </interleave>
    </element>
  </define>
  <define name="imml.header.url.attribute">
    <interleave>
      <attribute name="url">
        <ref name="imml.header.url.datatype"/>
      </attribute>
      <empty/>
    </interleave>
  </define>
  <!--
    End of Header #
    # # # # # # # #
  -->
  <define name="imml.message.content">
    <choice>
      <ref name="imml.book"/>
      <ref name="imml.reporting"/>
    </choice>
  </define>
  <define name="imml.book">
    <a:documentation>Book ##
</a:documentation>
    <element name="book">
      <interleave>
        <ref name="imml.book.attributes"/>
        <ref name="imml.book.metadata"/>
        <ref name="imml.book.assets"/>
        <ref name="imml.offer"/>
      </interleave>
    </element>
  </define>
  <define name="imml.book.attributes">
    <interleave>
    <attribute name="ean">
      <ref name="imml.offer.ean.datatype"/>
    </attribute>
    <optional>
      <attribute name="uid">
        <ref name="imml.book.id.datatype"/>
      </attribute>
    </optional>
    </interleave>
  </define>
  <define name="imml.book.metadata">
    <a:documentation/>
    <!-- Book metadata -->
    <a:documentation/>
    <element name="metadata">
      <interleave>
        <ref name="imml.book.metadata.title"/>
        <ref name="imml.book.metadata.language"/>
        <optional>
          <ref name="imml.book.metadata.subtitle"/>
        </optional>
        <optional>
          <ref name="imml.book.metadata.collection"/>
        </optional>
        <optional>
          <ref name="imml.book.metadata.pubdate"/>
        </optional>
        <ref name="imml.book.metadata.publisher"/>
        <ref name="imml.book.metadata.contributors"/>
        <ref name="imml.book.metadata.topics"/>
        <ref name="imml.book.metadata.description"/>
      </interleave>
    </element>
  </define>
  <define name="imml.book.metadata.title.attributes">
    <empty/>
  </define>
  <define name="imml.book.metadata.subtitle.attributes">
    <empty/>
  </define>
  <define name="imml.book.metadata.contributors.attributes">
    <empty/>
  </define>
  <!-- Work title -->
  <define name="imml.book.metadata.title">
    <element name="title">
      <ref name="imml.book.metadata.title.attributes"/>
      <text/>
    </element>
  </define>
  <!-- Work subtitle -->
  <define name="imml.book.metadata.subtitle">
    <element name="subtitle">
      <ref name="imml.book.metadata.subtitle.attributes"/>
      <text/>
    </element>
  </define>
  <!-- First publication date for this work -->
  <define name="imml.book.metadata.pubdate.attributes">
    <empty/>
  </define>
  <define name="imml.book.metadata.pubdate">
    <element name="publication">
      <ref name="imml.book.metadata.pubdate.attributes"/>
      <data type="date"/>
    </element>
  </define>
  <define name="imml.book.metadata.publisher.attributes">
    <interleave>
      <optional>
        <attribute name="uid">
          <ref name="imml.book.metadata.publisher.id.datatype"/>
        </attribute>
      </optional>
    </interleave>
  </define>
  <!-- Book current publisher -->
  <define name="imml.book.metadata.publisher">
    <element name="publisher">
      <ref name="imml.book.metadata.publisher.attributes"/>
      <text/>
    </element>
  </define>
  <!-- Book collection -->
  <define name="imml.book.metadata.collection">
    <element name="collection">
      <interleave>
        <optional>
          <attribute name="uid">
            <ref name="imml.book.metadata.collection.id.datatype"/>
          </attribute>
        </optional>
        <text/>
      </interleave>
    </element>
  </define>
  <!-- Book language -->
  <define name="imml.book.metadata.language">
    <choice>
      <element name="language">
        <ref name="imml.book.metadata.language.datatype"/>
      </element>
      <attribute name="lang">
        <ref name="imml.book.metadata.language.datatype"/>
      </attribute>
    </choice>
  </define>
  <!-- Book contributors -->
  <define name="imml.book.metadata.contributors">
    <element name="contributors">
      <ref name="imml.book.metadata.contributors.attributes"/>
      <oneOrMore>
        <ref name="imml.book.metadata.contributor"/>
      </oneOrMore>
    </element>
  </define>
  <define name="imml.book.metadata.contributor">
    <element name="contributor">
      <interleave>
        <optional>
          <attribute name="uid">
            <ref name="imml.book.contributor.id.datatype"/>
          </attribute>
        </optional>
        <ref name="imml.book.metadata.contributor.name"/>
        <ref name="imml.book.metadata.contributor.role"/>
      </interleave>
    </element>
  </define>
  <define name="imml.book.metadata.contributor.name">
    <element name="name">
      <text/>
    </element>
  </define>
  <define name="imml.book.metadata.contributor.role">
    <choice>
      <ref name="imml.book.metadata.contributor.role.supported"/>
      <ref name="imml.book.metadata.contributor.role.unsupported"/>
    </choice>
  </define>
  <define name="imml.book.metadata.contributor.role.supported">
    <element name="role">
      <interleave>
        <ref name="imml.book.contributor.datatype"/>
        <ref name="imml.supported"/>
      </interleave>
    </element>
  </define>
  <define name="imml.book.metadata.contributor.role.unsupported">
    <element name="role">
      <ref name="imml.unsupported"/>
    </element>
  </define>
  <!-- Book topics -->
  <define name="imml.book.metadata.topics">
    <choice>
      <ref name="imml.book.metadata.topics.supported"/>
      <ref name="imml.book.metadata.topics.unsupported"/>
    </choice>
  </define>
  <define name="imml.book.metadata.topics.supported">
    <element name="topics">
      <interleave>
        <oneOrMore>
          <ref name="imml.book.metadata.topic"/>
        </oneOrMore>
        <ref name="imml.supported"/>
      </interleave>
    </element>
  </define>
  <define name="imml.book.metadata.topics.unsupported">
    <element name="topics">
      <ref name="imml.unsupported"/>
    </element>
  </define>
  <define name="imml.book.metadata.topic">
    <element name="topic">
      <choice>
        <ref name="imml.book.metadata.topic.bisac"/>
        <ref name="imml.book.metadata.topic.clil"/>
        <ref name="imml.book.metadata.topic.keyword"/>
      </choice>
    </element>
  </define>
  <define name="imml.book.metadata.topic.bisac">
    <choice>
      <ref name="imml.book.metadata.topic.bisac.supported"/>
      <ref name="imml.book.metadata.topic.bisac.unsupported"/>
    </choice>
  </define>
  <define name="imml.book.metadata.topic.bisac.supported">
    <interleave>
      <attribute name="type">
        <value>bisac</value>
      </attribute>
      <ref name="imml.supported"/>
      <ref name="imml.bisac.id.datatype"/>
    </interleave>
  </define>
  <define name="imml.book.metadata.topic.bisac.unsupported">
    <interleave>
      <attribute name="type">
        <value>bisac</value>
      </attribute>
      <ref name="imml.unsupported"/>
    </interleave>
  </define>
  <define name="imml.book.metadata.topic.clil">
    <choice>
      <ref name="imml.book.metadata.topic.clil.supported"/>
      <ref name="imml.book.metadata.topic.clil.unsupported"/>
    </choice>
  </define>
  <define name="imml.book.metadata.topic.clil.supported">
    <interleave>
      <attribute name="type">
        <value>clil</value>
      </attribute>
      <ref name="imml.supported"/>
      <ref name="imml.clil.id.datatype"/>
    </interleave>
  </define>
  <define name="imml.book.metadata.topic.clil.unsupported">
    <interleave>
      <attribute name="type">
        <value>clil</value>
      </attribute>
      <ref name="imml.unsupported"/>
    </interleave>
  </define>
  <define name="imml.book.metadata.topic.keyword">
    <choice>
      <ref name="imml.book.metadata.topic.keyword.supported"/>
      <ref name="imml.book.metadata.topic.keyword.unsupported"/>
    </choice>
  </define>
  <define name="imml.book.metadata.topic.keyword.supported">
    <interleave>
      <attribute name="type">
        <value>keyword</value>
      </attribute>
      <ref name="imml.supported"/>
      <ref name="imml.keyword.id.datatype"/>
    </interleave>
  </define>
  <define name="imml.book.metadata.topic.keyword.unsupported">
    <interleave>
      <attribute name="type">
        <value>keyword</value>
      </attribute>
      <ref name="imml.unsupported"/>
    </interleave>
  </define>
  <!-- Book description -->
  <define name="imml.book.metadata.description.attributes">
    <interleave>
      <optional>
        <attribute name="lang">
          <a:documentation>Default to book language (although ideally bookseller preferred language)</a:documentation>
          <ref name="imml.book.metadata.language.datatype"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="extent">
          <choice>
            <value>short</value>
            <value>long</value>
            <value>140</value>
          </choice>
        </attribute>
      </optional>
    </interleave>
  </define>
  <define name="imml.book.metadata.description">
    <element name="description">
      <choice>
        <ref name="imml.book.metadata.description.content.xhtml"/>
        <ref name="imml.book.metadata.description.content.text"/>
      </choice>
    </element>
  </define>
  <define name="imml.book.metadata.description.content.xhtml">
    <choice>
      <ref name="imml.book.metadata.description.xhtml.supported"/>
      <ref name="imml.book.metadata.description.xhtml.unsupported"/>
    </choice>
  </define>
  <define name="imml.book.metadata.description.xhtml.supported">
    <interleave>
      <ref name="imml.book.metadata.description.attributes"/>
      <attribute name="format">
        <value>xhtml</value>
      </attribute>
      <ref name="imml.supported"/>
      <ref name="anyXHTMLElement"/>
    </interleave>
  </define>
  <define name="imml.book.metadata.description.xhtml.unsupported">
    <interleave>
      <optional>
        <ref name="imml.book.metadata.description.attributes"/>
        <attribute name="format">
          <value>xhtml</value>
        </attribute>
      </optional>
      <ref name="imml.unsupported"/>
    </interleave>
  </define>
  <define name="imml.book.metadata.description.content.text">
    <ref name="imml.book.metadata.description.attributes"/>
    <text/>
  </define>
  <define name="imml.book.assets">
    <a:documentation/>
    <!-- Book assets -->
    <a:documentation/>
    <element name="assets">
      <interleave>
        <ref name="imml.book.assets.cover"/>
        <zeroOrMore>
          <ref name="imml.book.assets.extract"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="imml.book.assets.full"/>
        </zeroOrMore>
      </interleave>
    </element>
  </define>
  <define name="imml.book.assets.mandatory_attr.common">
    <attribute name="mimetype">
      <ref name="imml.book.assets.mimetype.datatype"/>
    </attribute>
  </define>
  <define name="imml.book.assets.optional_attr.common">
    <interleave>
      <optional>
        <attribute name="size">
          <data type="integer">
            <param name="minInclusive">0</param>
          </data>
        </attribute>
      </optional>
      <optional>
        <attribute name="url">
          <ref name="imml.book.assets.url.datatype"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="checksum">
          <data type="token">
            <param name="pattern">\S*</param>
          </data>
        </attribute>
      </optional>
      <optional>
        <attribute name="last_modified">
          <data type="dateTime"/>
        </attribute>
      </optional>
    </interleave>
  </define>
  <define name="imml.book.assets.cover">
    <a:documentation>Cover files are made available to the receiving reseller via an ad hoc url</a:documentation>
    <element name="cover">
      <interleave>
        <ref name="imml.book.assets.optional_attr.common"/>
        <ref name="imml.book.assets.mandatory_attr.common"/>
      </interleave>
    </element>
  </define>
  <define name="imml.book.assets.extract">
    <a:documentation>Extract files are made available to the receiving reseller via an ad hoc url</a:documentation>
    <element name="extract">
      <choice>
        <ref name="imml.book.assets.extract.supported"/>
        <ref name="imml.book.assets.extract.unsupported"/>
      </choice>
    </element>
  </define>
  <define name="imml.book.assets.extract.supported">
    <interleave>
      <ref name="imml.book.assets.extract.content"/>
      <ref name="imml.supported"/>
    </interleave>
  </define>
  <define name="imml.book.assets.extract.unsupported">
    <ref name="imml.unsupported"/>
  </define>
  <define name="imml.book.assets.extract.content">
    <interleave>
      <ref name="imml.book.assets.mandatory_attr.common"/>
      <ref name="imml.book.assets.optional_attr.common"/>
    </interleave>
  </define>
  <define name="imml.book.assets.full">
    <a:documentation>Content files are made available via an ad hoc url tailored for the receiving warehouse</a:documentation>
    <element name="full">
      <choice>
        <ref name="imml.book.assets.full.supported"/>
        <ref name="imml.book.assets.full.unsupported"/>
      </choice>
    </element>
  </define>
  <define name="imml.book.assets.full.supported">
    <interleave>
      <ref name="imml.book.assets.full.content"/>
      <ref name="imml.supported"/>
    </interleave>
  </define>
  <define name="imml.book.assets.full.unsupported">
    <ref name="imml.unsupported"/>
  </define>
  <define name="imml.book.assets.full.content">
    <interleave>
      <ref name="imml.book.assets.mandatory_attr.common"/>
      <ref name="imml.book.assets.optional_attr.common"/>
    </interleave>
  </define>
  <define name="imml.offer">
    <a:documentation/>
    <!-- Book offer -->
    <a:documentation/>
    <element name="offer">
      <interleave>
        <optional>
          <!-- =ex-support -->
          <ref name="imml.offer.medium"/>
        </optional>
        <optional>
          <ref name="imml.offer.manifestation"/>
        </optional>
        <optional>
          <ref name="imml.offer.pagination"/>
        </optional>
        <optional>
          <ref name="imml.offer.alternatives"/>
        </optional>
        <!-- if not ready for sale, allow price to be missing -->
        <choice>
          <element name="ready_for_sale">
            <value>false</value>
          </element>
          <interleave>
            <element name="ready_for_sale">
              <data type="boolean"/>
            </element>
            <ref name="imml.offer.prices"/>
          </interleave>
        </choice>
        <ref name="imml.offer.sales_start_at"/>
        <optional>
          <ref name="imml.offer.sales_models"/>
        </optional>
      </interleave>
    </element>
  </define>
  <define name="imml.offer.sales_models">
    <a:documentation>Various types of sales models</a:documentation>
    <choice>
      <ref name="imml.offer.sales_models.supported"/>
      <ref name="imml.offer.sales_models.unsupported"/>
    </choice>
  </define>
  <define name="imml.offer.sales_models.supported">
    <element name="sales_models">
      <interleave>
        <oneOrMore>
          <ref name="imml.offer.sales_model"/>
        </oneOrMore>
        <ref name="imml.supported"/>
      </interleave>
    </element>
  </define>
  <define name="imml.offer.sales_models.unsupported">
    <element name="sales_models">
      <ref name="imml.unsupported"/>
    </element>
  </define>
  <define name="imml.offer.sales_model">
    <element name="sales_model">
      <interleave>
        <attribute name="type">
          <choice>
            <value>purchase</value>
            <value>subscription</value>
            <value>freemium</value>
          </choice>
        </attribute>
        <attribute name="available">
          <data type="boolean"/>
        </attribute>
        <attribute name="customer">
          <choice>
            <value>individual</value>
            <value>library</value>
          </choice>
        </attribute>
        <attribute name="format">
          <choice>
            <value>epub</value>
            <value>pdf</value>
            <value>bundle</value>
            <value>streaming</value>
          </choice>
        </attribute>
        <attribute name="protection">
          <choice>
            <value>none</value>
            <value>watermark</value>
            <value>drm</value>
          </choice>
        </attribute>
      </interleave>
    </element>
  </define>
  <define name="imml.offer.medium">
    <a:documentation>Book medium (for now, printed|digital)</a:documentation>
    <element name="medium">
      <ref name="imml.offer.medium.datatype"/>
    </element>
  </define>
  <define name="imml.offer.manifestation">
    <a:documentation>Manifestation</a:documentation>
    <element name="format">
      <ref name="imml.offer.manifestation.datatype"/>
    </element>
  </define>
  <define name="imml.offer.pagination.attributes">
    <empty/>
  </define>
  <define name="imml.offer.pagination">
    <a:documentation>Pagination of whatever alternative format where it makes sense</a:documentation>
    <element name="pagination">
      <ref name="imml.offer.pagination.attributes"/>
      <ref name="imml.offer.pagination.datatype"/>
    </element>
  </define>
  <define name="imml.offer.alternatives">
    <a:documentation>Alternative offers for the same content</a:documentation>
    <element name="alternatives">
      <oneOrMore>
        <ref name="imml.offer.alternative"/>
      </oneOrMore>
    </element>
  </define>
  <define name="imml.offer.alternative">
    <element name="alternative">
      <interleave>
        <attribute name="ean">
          <ref name="imml.offer.ean.datatype"/>
        </attribute>
        <optional>
          <attribute name="medium">
            <ref name="imml.offer.medium.datatype"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="format">
            <ref name="imml.offer.manifestation.datatype"/>
          </attribute>
        </optional>
      </interleave>
    </element>
  </define>
  <define name="imml.offer.sales_start_at">
    <a:documentation>Date de mise Ã  disposition</a:documentation>
    <choice>
      <ref name="imml.offer.sales_start_at.supported"/>
      <ref name="imml.offer.sales_start_at.unsupported"/>
    </choice>
  </define>
  <define name="imml.offer.sales_start_at.supported">
    <element name="sales_start_at">
      <interleave>
        <ref name="imml.supported"/>
        <data type="date"/>
      </interleave>
    </element>
